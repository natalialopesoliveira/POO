import java.math.*;

public interface ConstantesMatematicas {
	final static public double RAIZDE2 = 1.4142135623730950488;
	final static public double RAIZDE3 = 1.7320508075688772935;
	final static public double RAIZDE5 = 2.2360679774997896964;
	final static public double RAIZDE6 = RAIZDE2 * RAIZDE3;
	
	BigDecimal _RAIZDE2 = new BigDecimal(RAIZDE2);
	BigDecimal _RAIZDE3 = new BigDecimal(RAIZDE3);
	BigDecimal _RAIZDE5 = new BigDecimal(RAIZDE5);
	BigDecimal _RAIZDE6 = new BigDecimal(RAIZDE6);
	
	_RAIZDE2 = _RAIZDE2.setScale(19, BigDecimal.ROUND_HALF_UP);
	_RAIZDE3 = _RAIZDE3.setScale(19, BigDecimal.ROUND_HALF_UP);
	_RAIZDE5 = _RAIZDE5.setScale(19, BigDecimal.ROUND_HALF_UP);
	_RAIZDE6 = _RAIZDE6.setScale(19, BigDecimal.ROUND_HALF_UP);
}
