package Aplicação;

import java.math.BigDecimal;

import Matemática.ConstantesMatematicas;

public class Uso implements ConstantesMatematicas {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		BigDecimal RAIZDE2 = new BigDecimal(1.4142135623730950488);
		BigDecimal RAIZDE3 = new BigDecimal(1.7320508075688772935);
		BigDecimal RAIZDE5 = new BigDecimal(2.2360679774997896964);
		BigDecimal RAIZDE6 = RAIZDE2.multiply(RAIZDE3);
		System.out.println("Raiz de 2: " + RAIZDE2);
		System.out.println("Raiz de 3: " + RAIZDE3);
		System.out.println("Raiz de 5: " + RAIZDE5);
		System.out.println("Raiz de 6: " + RAIZDE6);
	}
}